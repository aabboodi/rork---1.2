{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "npx rork start -p 31drnwnb6wykdjmpnomy8",
    "start-web": "npx rork start -p 31drnwnb6wykdjmpnomy8 --web",
    "start-web-dev": "DEBUG=expo* npx rork start -p 31drnwnb6wykdjmpnomy8 --web",
    "security-check": "audit-ci --config audit-ci.json && license-checker --onlyAllow 'MIT;Apache-2.0;BSD-2-Clause;BSD-3-Clause;ISC;CC0-1.0'",
    "security-audit": "node scripts/security-audit.js",
    "lint:security": "eslint . --config .eslintrc.security.js --max-warnings 0",
    "sast:semgrep": "semgrep --config=auto --json --output=semgrep-results.json .",
    "sast:semgrep-ci": "semgrep --config=auto --error --strict .",
    "sast:sonar": "sonar-scanner",
    "sast:retire": "retire --js --node --outputformat json --outputpath retire-results.json",
    "sast:snyk": "snyk test --json > snyk-results.json || true",
    "sast:jscpd": "jscpd --reporters json --output ./reports/",
    "sast:full": "npm run sast:semgrep && npm run sast:retire && npm run sast:snyk && npm run sast:jscpd",
    "sast:ci": "npm run sast:semgrep-ci && npm run sast:retire && npm run sast:snyk",
    "audit": "npm audit --audit-level moderate",
    "vulnerability-scan": "npm audit --audit-level low --json > security-report.json",
    "pre-commit": "npm run lint:security && npm run security-audit && npm run security-check && npm run sast:ci",
    "build:security": "npm run vulnerability-scan && npm run security-audit && npm run security-check && npm run sast:full",
    "ci:security": "npm run build:security && npm run lint:security && npm run sast:ci",
    "security:comprehensive": "npm run ci:security && npm run sast:sonar",
    "sast:analysis": "node scripts/sast-analysis.js",
    "security:full-scan": "npm run sast:analysis",
    "security:integration": "node scripts/security-integration.js",
    "security:complete": "npm run security:integration"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@nkzw/create-context-hook": "1.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "7.1.6",
    "@shopify/flash-list": "1.7.6",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "audit-ci": "^7.1.0",
    "eslint": "^9.30.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^1.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eventemitter3": "^5.0.1",
    "expo": "^53.0.22",
    "expo-application": "~6.1.5",
    "expo-av": "~15.1.7",
    "expo-blur": "~14.1.5",
    "expo-camera": "~16.1.11",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "^16.0.1",
    "expo-crypto": "~14.1.5",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "^6.3.1",
    "expo-local-authentication": "~16.0.5",
    "expo-location": "~18.1.6",
    "expo-media-library": "~17.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.4",
    "expo-screen-capture": "~7.2.0",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "^0.10.3",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "jscpd": "^3.2.1",
    "license-checker": "^25.0.1",
    "lucide-react-native": "^0.542.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "retire": "5.2.8",
    "snyk": "1.1291.0",
